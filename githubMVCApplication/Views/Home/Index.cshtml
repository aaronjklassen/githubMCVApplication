@{
    ViewBag.Title = Model.User.Login;
}

<div>
    <img src="@Model.User.AvatarUrl" />
    <h1>@Model.User.Name</h1>
    <h4>Popular Repositories</h4>
    <ul>
        @foreach(var repository in Model.Repositories)
        {
            <li><a href="@repository.HtmlUrl">@repository.FullName</a></li>
        }
    </ul>
</div>